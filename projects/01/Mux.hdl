/**
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise

 * if sel == 0
   a
 else
   b
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=and1);
    And(a=sel, b=b, out=and2);
    Or(a=and1, b=and2, out=out);
}
